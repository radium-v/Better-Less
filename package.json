{
  "name": "better-less",
  "displayName": "Better Less",
  "version": "0.4.3",
  "description": "Cross-compatible syntax highlighting for Less",
  "publisher": "radium-v",
  "categories": [
    "Languages"
  ],
  "keywords": [
    "language",
    "less",
    "grammar",
    "css",
    "syntax"
  ],
  "repository": "https://github.com/radium-v/better-less",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0",
    "vscode": "^1.12.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "less",
        "aliases": [
          "Better Less",
          "less"
        ],
        "extensions": [
          ".less"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "less",
        "scopeName": "source.less",
        "path": "./syntaxes/Better Less.tmLanguage"
      }
    ]
  }
}
